<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PasswordEditText">
        <attr name="useStrikeThrough" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DrawClickableTextView">
        <attr name="isOpen" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference"/>
        <attr name="errorView" format="reference"/>
        <attr name="emptyView" format="reference"/>
        <attr name="noNetworkView" format="reference"/>
        <attr name="contentView" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TagTextView">
        <attr name="borderColor" format="color|reference" />
        <attr name="backgroundColor" format="color|reference" />
    </declare-styleable>
</resources>
